@{
    ViewBag.Title = "Encrypted";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="row">
    <div class="col-12">
        <h3>TestDB Encrypted</h3>
        <table class="table table-responsive table-striped">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Password</th>
                    <th>E-Mail</th>
                    <th>Phone</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var contact in Model.ContactsEncrypted) {
                    <tr>
                        <td>
                            @contact.FirstName
                        </td>
                        <td>
                            @contact.LastName
                        </td>
                        <td>
                            @contact.Password
                        </td>
                        <td>
                            @contact.Email
                        </td>
                        <td>
                            @contact.Phone
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>